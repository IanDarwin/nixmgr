#!/bin/sh

set -x

# test that unknown account failure works
env DEVICE_URI=usermgmt://foo/ cupsaccountfilter.py 123 jjjjjjj test.ps 1 - testdata

# test that account balance failure works
env DEVICE_URI=usermgmt://foo/ cupsaccountfilter.py 123 testfail test.ps 1 - testdata

# test that we get as far as trying the next filter
env DEVICE_URI=usermgmt://NOSUCHFILTER/p1 cupsaccountfilter.py 123 ian test.ps 1 - testdata
